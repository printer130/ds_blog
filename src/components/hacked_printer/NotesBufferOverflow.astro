---
import Title from '@components/hacked_printer/Title.astro'
import { getCollection } from 'astro:content'
import { BUFFER_OVERFLOW } from '../../consts'
import Link from './Link.astro'

const topOverFLow = (
	await getCollection('hacked_printer', ({ id }) => {
		return id.startsWith(BUFFER_OVERFLOW)
	})
).sort((a, b) => a.data.pubDate.valueOf() - b.data.pubDate.valueOf())
---

<section>
	<Title>Buffer Overflow</Title>
	<ul>
		{
			topOverFLow.map((post) => {
				return <Link post={post} />
			})
		}
	</ul>
</section>
