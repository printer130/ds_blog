---
import Title from '@components/redteam/Title.astro'
import { getCollection } from 'astro:content'
import { ENTRIES } from 'src/consts'
import Link from './Link.astro'

const generic = (
	await getCollection('redteam', ({ id }) => {
		return id.startsWith(ENTRIES.SOCIAL_ENG_ATTACK_VECTORS)
	})
).sort((a, b) => a.data.pubDate.valueOf() - b.data.pubDate.valueOf())
console.log(generic)
---

<section>
	<Title>Vectores de ataque en ingenieria social</Title>
	<ul>
		{
			generic.map((post) => {
				return <Link post={post} />
			})
		}
	</ul>
</section>
