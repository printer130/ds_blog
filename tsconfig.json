{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "jsx": "react-jsx",
    "allowJs": true,
    "strictNullChecks": true,
    "jsxImportSource": "preact",
    "types": [
      "astro/client",
      "preact"
    ],
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@components/*": [ "src/components/*" ],
      "@icons/*": [ "src/icons/*" ],
      "@hooks/*": [ "src/hooks/*" ],
      "@layouts/*": [ "src/layouts/*" ],
      "@services/*": [ "src/services/*" ],
      "@utils/*": [ "src/utils/*" ],
      "@assets/*": [ "assets/*" ],
      "@styles/*": [ "styles/*" ],
      "@CONSTS/*": [ "CONSTS/*" ],
      "@/*":["src/*"]
    },
  },
}